{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Xml1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "LS_ABLB",
						"type": "LinkedServiceReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     validationMode: 'none',",
				"     namespaces: true) ~> source1",
				"source1 sink(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     entity: '',",
				"     format: 'cdm',",
				"     manifestType: 'manifest',",
				"     folderPath: '',",
				"     fileSystem: '',",
				"     columnNamesAsHeader: false,",
				"     columnDelimiter: ',',",
				"     quoteChar: '\\\"',",
				"     escapeChar: '\\\\',",
				"     truncate: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}